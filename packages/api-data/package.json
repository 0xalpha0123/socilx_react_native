{
  "name": "@socialx/api-data",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "transfer:sea": "cp ./src/extensions/sea.js ./dist/extensions/sea.js",
    "build": "tsc && yarn transfer:sea",
    "test": "jest --runInBand --onlyChanged --detectOpenHandles",
    "test:all": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --silent"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "react-native": "^0.56.0"
  },
  "dependencies": {
    "@socialx/commons": "^0.0.1",
    "gun": "0.9.9999991",
    "gun-flint": "^0.0.28",
    "js-base64": "^2.4.9",
    "utility-types-fixme-todo": "latest",
    "uuid": "^3.3.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react-native": "^0.56.24",
    "@types/yup": "^0.24.9",
    "jest": "^23.5.0",
    "moment": "^2.22.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
